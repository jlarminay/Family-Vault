{
  "name": "nuxt-app",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm i && node start.js --dev",
    "build": "nuxt build --dotenv .env.production",
    "dev": "nuxt dev --host",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "docker": "docker-compose -f docker-compose.yml up -d",
    "db:studio": "prisma studio --browser none",
    "db:reset": "prisma migrate reset --skip-seed -f",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "prisma migrate reset -f",
    "db:migrate": "prisma migrate dev --name",
    "test": "playwright test",
    "s3-monitor": "node --loader ts-node/esm tools/s3Monitor.ts"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seeds/index.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.478.0",
    "@aws-sdk/lib-storage": "^3.481.0",
    "@meforma/vue-toaster": "^1.3.0",
    "@pinia/nuxt": "^0.5.1",
    "@prisma/client": "^5.15.1",
    "@quasar/extras": "^1.16.11",
    "@tailwindcss/container-queries": "^0.1.1",
    "@trpc/client": "^10.44.1",
    "@trpc/server": "^10.44.1",
    "@types/leaflet": "^1.9.12",
    "@types/ua-parser-js": "^0.7.39",
    "chart.js": "^4.4.3",
    "glightbox": "^3.3.0",
    "image-size": "^1.1.1",
    "leaflet": "^1.9.4",
    "leaflet.heat": "^0.2.0",
    "lightgallery": "^2.7.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "pinia": "^2.1.7",
    "pm2": "^5.4.1",
    "quasar": "^2.14.7",
    "shelljs": "^0.8.5",
    "trpc-nuxt": "^0.10.21",
    "ua-parser-js": "^1.0.38",
    "validator": "^13.11.0",
    "vue-chartjs": "^5.3.1",
    "vue-markdown-render": "^2.2.1",
    "vuewordcloud": "^19.0.0",
    "zod": "^3.22.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nuxt/devtools": "latest",
    "@nuxtjs/tailwindcss": "^6.10.4",
    "@playwright/test": "^1.40.1",
    "@sidebase/nuxt-auth": "^0.6.7",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.8",
    "@types/shelljs": "^0.8.15",
    "@types/validator": "^13.11.10",
    "concurrently": "^8.2.2",
    "dayjs-nuxt": "^2.1.9",
    "next-auth": "4.22.5",
    "npm-check-updates": "^16.14.20",
    "nuxt": "^3.8.2",
    "nuxt-quasar-ui": "^2.0.8",
    "prettier": "^3.1.1",
    "prisma": "^5.15.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vue": "^3.3.13",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.27"
  }
}
