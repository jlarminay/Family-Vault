{
  "name": "nuxt-app",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm i && node start.js --dev",
    "build": "nuxt build --dotenv .env.production",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "docker": "docker-compose -f docker-compose.yml up -d",
    "db:reset": "prisma migrate reset --skip-seed -f",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "prisma migrate reset -f",
    "db:migrate": "prisma migrate dev --name",
    "test": "playwright test"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seeds/index.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.478.0",
    "@aws-sdk/lib-storage": "^3.481.0",
    "@meforma/vue-toaster": "^1.3.0",
    "@pinia/nuxt": "^0.5.1",
    "@prisma/client": "^5.7.1",
    "@quasar/extras": "^1.16.9",
    "@trpc/client": "^10.44.1",
    "@trpc/server": "^10.44.1",
    "@types/shelljs": "^0.8.15",
    "image-size": "^1.1.1",
    "node-fetch": "^3.3.2",
    "pinia": "^2.1.7",
    "quasar": "^2.14.2",
    "shelljs": "^0.8.5",
    "trpc-nuxt": "^0.10.15",
    "validator": "^13.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxtjs/tailwindcss": "^6.10.1",
    "@playwright/test": "^1.40.1",
    "@sidebase/nuxt-auth": "^0.6.3",
    "@types/node": "^20.10.5",
    "@types/validator": "^13.11.7",
    "concurrently": "^8.2.2",
    "dayjs-nuxt": "^2.1.9",
    "next-auth": "4.22.5",
    "npm-check-updates": "^16.14.20",
    "nuxt": "^3.8.2",
    "nuxt-quasar-ui": "^2.0.7",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vue": "^3.3.12",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.25"
  }
}
