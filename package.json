{
  "name": "nuxt-app",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "npm i && node start.js --dev",
    "build": "nuxt build --dotenv .env.production",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "docker": "docker-compose -f docker-compose.yml up -d",
    "db:studio": "prisma studio --browser none",
    "db:reset": "prisma migrate reset --skip-seed -f",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "prisma migrate reset -f",
    "db:migrate": "prisma migrate dev --name",
    "generate-pwa-assets": "pwa-assets-generator",
    "test": "playwright test"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seeds/index.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.717.0",
    "@aws-sdk/lib-storage": "^3.717.0",
    "@formkit/auto-animate": "^0.8.2",
    "@meforma/vue-toaster": "^1.3.0",
    "@panzoom/panzoom": "^4.5.1",
    "@pinia/nuxt": "^0.9.0",
    "@prisma/client": "^6.1.0",
    "@quasar/extras": "^1.16.15",
    "@tailwindcss/container-queries": "^0.1.1",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "@types/leaflet": "^1.9.15",
    "@types/ua-parser-js": "^0.7.39",
    "chart.js": "^4.4.7",
    "gsap": "^3.12.5",
    "image-size": "^1.2.0",
    "leaflet": "^1.9.4",
    "lightgallery": "^2.8.2",
    "node-fetch": "^3.3.2",
    "panzoom": "^9.4.3",
    "pinia": "^2.3.0",
    "pm2": "^5.4.3",
    "quasar": "^2.17.5",
    "shelljs": "^0.8.5",
    "trpc-nuxt": "^0.10.22",
    "ua-parser-js": "^2.0.0",
    "validator": "^13.12.0",
    "vue-chartjs": "^5.3.2",
    "vuewordcloud": "^19.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "@nuxt/devtools": "latest",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@playwright/test": "^1.49.1",
    "@sidebase/nuxt-auth": "^0.10.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.2",
    "@types/shelljs": "^0.8.15",
    "@types/validator": "^13.12.2",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vite-pwa/nuxt": "^0.10.6",
    "concurrently": "^9.1.0",
    "dayjs-nuxt": "^2.1.11",
    "next-auth": "4.21.1",
    "npm-check-updates": "^17.1.12",
    "nuxt": "^3.14.1592",
    "nuxt-quasar-ui": "^2.1.7",
    "prettier": "^3.4.2",
    "prisma": "^6.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.1.10"
  }
