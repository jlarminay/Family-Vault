{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "node start.js --dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "docker": "docker-compose -f docker-compose.yml up -d",
    "db:reset": "prisma migrate reset --skip-seed -f",
    "db:seed": "prisma migrate reset -f",
    "db:migrate": "prisma migrate dev --name"
  },
  "prisma": {
    "seed": "ts-node-esm prisma/seeds/index.ts"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.5.1",
    "@prisma/client": "^5.7.1",
    "@quasar/extras": "^1.16.9",
    "@trpc/client": "^10.44.1",
    "@trpc/server": "^10.44.1",
    "pinia": "^2.1.7",
    "quasar": "^2.14.2",
    "trpc-nuxt": "^0.10.15",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxtjs/tailwindcss": "^6.10.1",
    "@types/node": "^20.10.5",
    "concurrently": "^8.2.2",
    "nuxt": "^3.8.2",
    "nuxt-quasar-ui": "^2.0.7",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vue": "^3.3.12",
